{
  "name": "friends-without-benefits",
  "version": "0.1.0",
  "description": "Object Recognition Scavenger Hunt",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "setup": "npm run setup:server && npm run setup:client",
    "setup:server": "npm --prefix server install",
    "setup:client": "npm --prefix client install",
    "dev": "concurrently --kill-others-on-fail \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "NODE_ENV=staging npm --prefix server run dev",
    "dev:client": "npm --prefix client run dev",
    "build": "npm run build:server && npm run build:client",
    "build:server": "npm --prefix server run build",
    "build:client": "npm --prefix client run build",
    "lint:client": "npm --prefix client run lint",
    "start": "npm --prefix server run start",
    "build-and-start": "npm run build && npm run start",
    "start:client": "npm --prefix client run preview",
    "clean": "rm -rf node_modules dist server/node_modules client/node_modules",
    "render-build": "npm run setup && npm run build",
    "render:build-clean": "npm run clean && npm run render-build",
    "deploy-test-start": "concurrently \"cd server && npm run start\" \"npx cypress run --browser electron\"",
    "cypress:verify": "npx cypress verify",
    "cypress:open": "npx cypress open",
    "cypress:headless-run": "npx cypress run",
    "cypress:headless-e2e": "npx cypress run --e2e",
    "cypress:headless-component": "npx cypress run --component"
  },
  "author": "Thomas Stemler, Souad Hassen-Bouzouita, Dario Zambrano, Greg Barker",
  "license": "MIT",
  "dependencies": {
    "apollo-client": "^2.6.10",
    "concurrently": "^9.1.0",
    "npm-run-all": "^4.1.5",
    "react-dom": "^18.3.1",
    "react-modal": "^3.16.3",
    "typewriter-effect": "^2.21.0"
  },
  "devDependencies": {
    "@testing-library/cypress": "^10.0.2",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.5",
    "@types/pretty": "^2.0.3",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.1.0",
    "cypress": "^13.17.0",
    "happy-dom": "^16.3.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "npm-run-all": "^4.1.5",
    "pretty": "^2.0.0",
    "react": "^18.3.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "vite-plugin-static-copy": "^2.2.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^2.1.8"
  },
  "include": [
    "server/**/*",
    "client/**/*",
    "cypress/**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}
