{
  "name": "friends-without-benefits",
  "version": "0.1.0",
  "description": "Object Recognition Scavenger Hunt",
  "main": "index.js",
  "scripts": {
    "install:all": "npm-run-all --parallel install:server install:client",
    "install:server": "npm --prefix server install",
    "install:client": "npm --prefix client install",
    "dev": "concurrently --kill-others-on-fail \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "npm --prefix server run watch",
    "dev:client": "npm --prefix client run dev",
    "build": "npm-run-all build:server build:client",
    "build:server": "npm --prefix server run build",
    "build:client": "npm --prefix client run build",
    "start": "concurrently --kill-others-on-fail \"npm run start:server\" \"npm run start:client\"",
    "start:server": "npm --prefix server run start",
    "start:client": "npm --prefix client run preview"
  },
  "author": "Thomas Stemler, Souad Hassen-Bouzouita, Dario Zambrano, Greg Barker",
  "license": "MIT",
  "dependencies": {
    "apollo-client": "^2.6.10"
  },
  "devDependencies": {
    "concurrently": "^9.1.0",
    "npm-run-all": "^4.1.5"
  }
}
